    <item>
      <title>How can we help?</title>
      <link>/loyall.ch/contact/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      
      <guid>/loyall.ch/contact/</guid>
      <description>&lt;script type=&#34;text/javascript&#34;&gt;
  var onloadCallback = function() {
    grecaptcha.render(&#39;g-recaptcha&#39;, {
      &#39;sitekey&#39;: &#39;6LeURygTAAAAAAfeKQjIzStD2zAPA3gRrxmA0oMK&#39;,
      &#39;callback&#39;: verifyRecaptchaCallback
    });
  };
&lt;/script&gt;
&lt;p&gt;Leave us a message and we will get back to you as soon as possible.&lt;/p&gt;

&lt;div class=&#34;mdl-grid&#34;&gt;
  &lt;div class=&#34;mdl-cell mdl-cell--8-col mdl-cell--8-col-tablet mdl-cell--12-col-phone&#34;&gt;
    &lt;form id=&#34;contact-form&#34; action=&#34;?&#34;&gt;
      &lt;div class=&#34;form-fields-group&#34;&gt;
        &lt;div class=&#34;mdl-textfield mdl-js-textfield mdl-textfield--floating-label&#34;&gt;
          &lt;input class=&#34;mdl-textfield__input&#34; type=&#34;text&#34; id=&#34;name&#34; pattern=&#34;[A-Z,a-z, ]*&#34; name=&#34;name&#34;&gt;
          &lt;label class=&#34;mdl-textfield__label&#34; for=&#34;name&#34;&gt;First and last name&lt;/label&gt;
          &lt;span class=&#34;mdl-textfield__error&#34;&gt;Letters and spaces only&lt;/span&gt;
        &lt;/div&gt;
      &lt;/div&gt;
      &lt;div class=&#34;form-fields-group&#34;&gt;
        &lt;div class=&#34;mdl-textfield mdl-js-textfield mdl-textfield--floating-label&#34;&gt;
          &lt;input class=&#34;mdl-textfield__input&#34; type=&#34;email&#34; id=&#34;email&#34; name=&#34;email&#34;&gt;
          &lt;label class=&#34;mdl-textfield__label&#34; for=&#34;email&#34;&gt;Email&lt;/label&gt;
          &lt;span class=&#34;mdl-textfield__error&#34;&gt;Invalid email address&lt;/span&gt;
        &lt;/div&gt;
      &lt;/div&gt;
      &lt;div class=&#34;mdl-textfield mdl-js-textfield mdl-textfield--floating-label&#34;&gt;
        &lt;textarea class=&#34;mdl-textfield__input&#34; type=&#34;text&#34; rows=&#34;5&#34; id=&#34;request&#34; name=&#34;request&#34;&gt;&lt;/textarea&gt;
        &lt;label class=&#34;mdl-textfield__label&#34; for=&#34;request&#34;&gt;Your message...&lt;/label&gt;
      &lt;/div&gt;
      &lt;p&gt;Note: All fields are required&lt;/p&gt;
      &lt;div class=&#34;g-recaptcha&#34; id=&#34;g-recaptcha&#34;&gt;&lt;/div&gt;

      &lt;div&gt;
        &lt;button class=&#34;mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--colored submit-button&#34; id=&#34;submit-button&#34; disabled&gt;
        Submit
      &lt;/button&gt;
    &lt;/div&gt;
    &lt;/form&gt;
    &lt;/div&gt;
    &lt;!--&lt;div class=&#34;mdl-cell mdl-cell--1-col mdl-cell--1-col-tablet mdl-cell--1-col-phone&#34;&gt;--&gt;
      
    &lt;!--&lt;/div&gt;--&gt;
    &lt;div class=&#34;mdl-cell mdl-cell--1-col mdl-cell--2-col-tablet mdl-cell--1-col-phone&#34;&gt;
      &lt;div class=&#34;mdl-card mdl-shadow--2dp&#34;&gt;
        &lt;div class=&#34;mdl-card__supporting-text&#34;&gt;
          &lt;h5&gt;Office address&lt;/h5&gt;
          &lt;p&gt;
            Loyall GmbH &lt;br&gt;
            Seestrasse 124 &lt;br&gt;
            CH-8802 Kilchberg (Zurich)&lt;br&gt;
          &lt;/p&gt;
          &lt;a href=&#34;https://goo.gl/maps/FTHi5CMEZxt&#34; target=&#34;_blank&#34;&gt;View on the map&lt;/a&gt;
        &lt;/div&gt;
      &lt;/div&gt;
    
    &lt;/div&gt;
    &lt;/div&gt;
    &lt;script src=&#34;https://www.google.com/recaptcha/api.js?onload=onloadCallback&amp;render=explicit&#34; async defer&gt;
    &lt;/script&gt;</description>
